{
  "name": "refire-hackathon-boilerplate",
  "version": "1.0.0",
  "description": "Boilerplate for quick prototyping with refire-app",
  "main": "index.js",
  "private": true,
  "scripts": {
    "clean": "rimraf dist",
    "start": "node devServer.js",
    "build:webpack": "NODE_ENV=production webpack --config webpack.config.prod.js",
    "build": "npm run clean && npm run build:webpack && npm run html",
    "html": "cp index.html dist/index.html",
    "login": "firebase login",
    "bootstrap": "firebase data:set / bootstrap.json",
    "deploy": "firebase deploy",
    "lint": "eslint src"
  },
  "author": "Lari Hoppula",
  "license": "MIT",
  "dependencies": {
    "lodash": "^3.10.1",
    "react": "^0.14.7",
    "react-dom": "^0.14.7",
    "refire-app": "^1.0.1"
  },
  "devDependencies": {
    "babel-core": "^6.4.5",
    "babel-eslint": "^5.0.0",
    "babel-loader": "^6.2.0",
    "babel-plugin-transform-object-rest-spread": "^6.5.0",
    "babel-preset-es2015": "^6.3.13",
    "babel-preset-react": "^6.3.13",
    "babel-preset-react-hmre": "^1.0.1",
    "css-loader": "^0.23.0",
    "eslint": "^1.10.3",
    "eslint-plugin-babel": "^3.0.0",
    "eslint-plugin-import": "^0.12.1",
    "eslint-plugin-react": "^3.16.1",
    "express": "^4.13.4",
    "extract-text-webpack-plugin": "^1.0.1",
    "file-loader": "^0.8.5",
    "json-loader": "^0.5.4",
    "react-tap-event-plugin": "^0.2.1",
    "rimraf": "^2.5.2",
    "style-loader": "^0.13.0",
    "url-loader": "^0.5.7",
    "webpack": "^1.12.12",
    "webpack-dev-middleware": "^1.5.1",
    "webpack-hot-middleware": "^2.6.4"
  }
}
